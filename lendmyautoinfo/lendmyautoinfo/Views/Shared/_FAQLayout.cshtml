<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="icon" href="@Url.Content("/Images/lendmyauto2.png")" />
    <title>@ViewBag.Title - FAQ</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <header id="headerPic" class="faq-img">
        <h1 style=" color:black; margin-right:60%;" class="heading contact-title">FAQ's</h1>
        <p></p>
    </header>
    <nav class="navbar navbar-expand-lg fixed-top navbar-light">

        <div class="container" style="margin-top:10px;">
            <a href="/Home"><img src="~/Images/lendmyauto2.png" style="width:70%;" /></a>
            <button id="NavTogglerLend" class="navbar-toggler" type="button" onclick="SolidNav()" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
        </div>
        <div class="container" style="margin-top:10px;">
            <div class="collapse navbar-collapse" id="navbarNav">
                <ul class="navbar-nav flex-fill w-100 flex-nowrap justify-content-end">
                    <li class="nav-item active">
                        <a class="nav-link" href="/Home">Home <span class="sr-only">(current)</span></a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/Home/About">About</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/Home/Sharing">Community of Car Sharers</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/Home/Contact">Contact Us</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/Home/FAQ">FAQ's</a>
                    </li>

                </ul>
            </div>
        </div>

    </nav>

    <div class="container body-content">
        @RenderBody()
        <hr />
        @{ Html.RenderPartial("_Footer", null, null); }
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/navbar")

    @RenderSection("scripts", required: false)
</body>
</html>

